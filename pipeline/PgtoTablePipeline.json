{
	"name": "PgtoTablePipeline",
	"properties": {
		"description": "suresh.test_suresh",
		"activities": [
			{
				"name": "CopyTabledata",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "PostgreSqlSource",
						"query": "select id,name,doj from suresh.test_suresh;"
					},
					"sink": {
						"type": "AzureTableSink",
						"azureTableInsertType": "merge",
						"azureTableDefaultPartitionKeyValue": "id",
						"writeBatchSize": 10000,
						"maxConcurrentConnections": 1
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "PgDataset",
						"type": "DatasetReference",
						"parameters": {
							"DBName": "suresh",
							"TableName": "test_suresh"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}